{
  "name": "experiment-msw-for-storybook",
  "version": "1.0.0",
  "main": "index.js",
  "author": "akky--xxxx <114.s.aki@gmail.com>",
  "engines": {
    "node": "18.12.0",
    "npm": "use \"yarn\" instead \"npm\"",
    "yarn": "1.22.19"
  },
  "license": "MIT",
  "scripts": {
    "check-all": "npm-run-all fix -p spell-check type-check -s test",
    "check-code": "run-p spell-check lint type-check",
    "fix": "run-p fix:* -c",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix --max-warnings 0",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-prettier fix-ts-by-eslint",
    "lint": "run-p lint:* -c",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings 0",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:editorconfig": "editorconfig-checker",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "storybook": "start-storybook",
    "test": "jest --coverage --silent",
    "type-check": "tsc"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@swc/core": "^1.3.10",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.0",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cspell": "^6.12.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-strict-check": "^0.0.9",
    "eslint-plugin-airbnb": "^0.0.1-security",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-strict-check": "^0.0.6",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^44.0.2",
    "fixpack": "^4.0.0",
    "http-status-codes": "^2.2.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
